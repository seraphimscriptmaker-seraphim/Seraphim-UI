local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Load Library
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/seraphimscriptmaker-seraphim/Seraphim-script-/refs/heads/main/Seraphim%20public"))()

-- Main Window
local window = library:AddWindow("SERAPHIM UI", {
    main_color = Color3.fromRGB(34, 56, 34),
    min_size = Vector2.new(600, 820),
    can_resize = false,
})
